# Production Environment Variables for REC_Website
# Copy this to .env.local for local testing with production backend

# Backend API URL (Railway deployment)
VITE_API_URL=https://rectify-production.up.railway.app

# Frontend URL (Netlify deployment)
VITE_FRONTEND_URL=https://your-netlify-app.netlify.app

# Environment
VITE_NODE_ENV=production

# Feature flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_DEBUG=false